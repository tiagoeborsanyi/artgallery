{
  "name": "artgallery",
  "version": "1.0.0",
  "description": "Art Gallery for sale illustrations and draws",
  "main": "index.js",
  "engine": {
    "node": "12.18.2",
    "npm": "6.14.5"
  },
  "scripts": {
    "start": "node index.js",
