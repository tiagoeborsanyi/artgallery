  "dependencies": {
    "body-parser": "1.19.0",
    "cookie-session": "1.4.0",
    "express": "4.17.1",
    "mongoose": "5.9.20"
  },
  "devDependencies": {
    "nodemon": "2.0.4"
  }
}
